#include <stdio.h>

int main() {
	// your code goes here
	int arr[100], n , search , i ,flag = 0;
	
	printf("Please enter the number of elements you want to insert in the array : \n");
	scanf("%d",&n);
	printf("Insert elements in your array : \n");
	for(i=0;i<n;i++){
	    scanf("%d",&arr[i]);
	}
	printf("Which element you want to search ? \nEnter : ");
	scanf("%d",&search);
	
	for(i=0;i<n;i++){
	    
	    if(arr[i]==search) {
	        flag = 1;
	        break;
	    }
	}
	
	(flag==1) ? printf("\nFound.At %d",i+1) : printf("\nNot found. :(");
	

}

